{
	"p4info": "../src/build/main.p4.p4info.txt",
	
	"bmv2_json": "../src/build/main.json",

	"network_info": "../src/topologies/linear/network_info.json",

	"table_entries": "../src/p4_table_entries.config",

	"start_nodes_strategy": "PRIORITIZE_OUTER",

	"devices":[
		{
			"name":"hsnort",
			"cmds":["touch ../snort/logs/eth0/output.pcap","chmod o+x ../snort/logs/eth0/output.pcap", "tshark -i eth0 -w ../snort/logs/eth0/output.pcap &",
				"snort -c ../snort/configuration/snort.lua --rule-path ../snort/rules/snort-community -A alert_json --lua \"alert_json = {file = true}\" -l ../snort/logs/eth0 -i eth0 > ../snort/logs/eth0/output.txt &",
			   "snort -c ../snort/configuration/snort.lua --rule-path ../snort/rules/snort-community -A alert_json --lua \"alert_json = {file = true}\" -l ../snort/logs/hsnort-eth1 -L log_pcap -i hsnort-eth1 > ../snort/logs/hsnort-eth1/output.txt &",
			   "snort -c ../snort/configuration/snort.lua --rule-path ../snort/rules/snort-community -A alert_json --lua \"alert_json = {file = true}\" -l ../snort/logs/hsnort-eth2 -L log_pcap -i hsnort-eth2 > ../snort/logs/hsnort-eth2/output.txt &"]
		},
		{
			"name":"hinternet",
			"cmds":["tcpreplay-edit --mtu-trunc -i eth0 -p1500 -L 30000 ../../CICIDS2017-PCAPS/Friday-WorkingHours.pcap"]
		}]

}